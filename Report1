processing code
-----------------------------------------------------------
import processing.serial.*;
  Serial p;
void setup(){
  p = new Serial(this,"COM3",9600);
}
void draw(){
  if(p.available()>0){
    String m = p.readStringUntil('\n');
    if(m!=null)
    print (m);
  }
}

----------------------------------------------------------

arduino code

----------------------------------------------------------

void setup(){
  Serial.begin(9600);
}
double th(int v) {
  double t;
  t = log(((10240000/v) - 10000));
  t = 1 /(0.001129148 + (0.000234125*t) + (0.0000000876741*t*t*t));
  t = t - 273.15;
  return t;
}
void loop(){
  int s= analogRead(th (s));
  delay(500);
}
